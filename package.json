{
  "name": "mern-project-typescript",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run server:build && npm run client:build",
    "docker:dev": "docker compose -f docker-compose.dev.yml up -d ",
    "docker:dev:build": "docker compose -f docker-compose.dev.yml up -d --build",
    "start": "ts-node backend/server.ts",
    "start:prod": "node  dist/backend/server.js",
    "server": " \"nodemon\"  ",
    "server:dev": "nodemon --watch backend --ext ts --exec \"ts-node -r tsconfig-paths/register backend/server.ts\"",
    "client": "npm run webpack --prefix frontend",
    "dev": "concurrently \"npm run server\"  \"npm run client\" ",
    "test": "jest --forceExit --detectOpenHandles",
    "seed": "ts-node seed/seed.ts",
    "lint:ts": "eslint \"./**/*.ts\"",
    "lint:ts:fix": "eslint \"**/*.ts\" --fix",
    "client:build": "npm install --prefix frontend && npm run build --prefix frontend",
    "server:build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "vercel-build": "rm -rf dist && npm run build"
  },
  "_moduleAliases": {
    "@": ".",
    "config": "./config",
    "constants": "./constants",
