{
  "version": 2,
  "installCommand": "npm install --production=false",
  "buildCommand": "npm run vercel-build",
  "devCommand": "npm run start:prod",
  "builds": [
    {
      "src": "dist/backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "dist/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/backend/server.js"
    }
  ]
}